    {
      "Tokenize" :
      {
	"class" : "STokenizationUttProc",
	"plug-in" : "tokenization_uttproc.spi",
	"features" : /* features of the utterance processor */
	{
	  "tokenizer symbols" :
	  {
	    "whitespace" : " \t\n\r",
	    "single-char" : "(){}[]",
	    "pre-punctuation" : "\"'`({[",
	    "post-punctuation" : "\"'`.,:;!?(){}[]"
	  }
	}
      },
      "Normalize" :
